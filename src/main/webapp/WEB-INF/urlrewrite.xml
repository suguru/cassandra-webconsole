<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>

	<rule>
		<from>^/$</from>
		<to last="true">/core/index</to>
	</rule>
	
	<rule>
		<from>^/(.*?)(.css|.js|.jpg|.png|.gif)$</from>
		<to last="true">/$1$2</to>
	</rule>
	
	<rule>
		<from>^/(.*)$</from>
		<to last="true">/core/$1</to>
	</rule>
	
</urlrewrite>
